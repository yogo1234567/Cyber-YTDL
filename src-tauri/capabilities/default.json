{
  "$schema": "https://schema.tauri.app/capability/2.0",
  "identifier": "default",
  "description": "capability for the main and mini window",
  "windows": [
    "main",
    "mini"
  ],
  "permissions": [
    "core:default",
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "shell:default",
    "shell:allow-open",
    "opener:default",
    "opener:allow-open-url",
    "core:window:allow-start-dragging",
    "core:window:allow-minimize",
    "core:window:allow-close",
    "clipboard-manager:allow-read-text",
    "clipboard-manager:allow-write-text",
    "clipboard-manager:default",
    "dialog:allow-open",
    "dialog:default",
    "fs:default",
    "fs:allow-write-text-file",
    "fs:allow-remove",
    "fs:allow-exists"
  ]
}